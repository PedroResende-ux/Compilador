procedure Main is
begin
  x := 10;
  
  if x > 5 then
    Put_Line("x is greater than 5");
  
  if x < 20 then
    Put_Line("x is less than 20")
  else
    Put_Line("x is not less than 20");
  
  if x = 10 then
    Put_Line("x equals 10");
  
  if x /= 15 then
    Put_Line("x does not equal 15")
end Main;
